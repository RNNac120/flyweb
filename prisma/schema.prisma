generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aviao {
  matricula           String   @id    // A chave primária é a matrícula
  horas_voadas        Int
  data_prox_mnt       DateTime
  fabricante          String
  modelo_anv          String
  disp_voo            Boolean
  num_hangar          Int
}
